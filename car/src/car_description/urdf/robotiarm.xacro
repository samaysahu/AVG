      <origin rpy="1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0.138" />
      <geometry>
        <mesh filename="package://car_description/meshes/upperarm.dae" />
      </geometry>
      <material name="LightGrey">
        <color rgba="0.7 0.7 0.7 1.0" />
      </material>
    </visual>
    <collision>
      <origin rpy="1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0.138" />
      <geometry>
        <mesh filename="package://car_description/meshes/upperarm.dae" />
      </geometry>
    </collision>
    <inertial>
      <mass value="8.393"/>
