      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <!-- Joints -->
  <!-- Base joint fixed to arm_world -->
  <joint name="base_fixed_joint" type="fixed">
    <parent link="arm_world"/>
    <child link="base_link"/>
    <origin xyz="3 0 0.075" rpy="0 0 0"/>
  </joint>

  <joint name="joint_1" type="revolute">
    <parent link="base_link" />
    <child link="shoulder_link" />
    <origin rpy="0 0 0" xyz="0 0 0.1625" />
    <axis xyz="0 0 1" />
    <limit effort="150.0" lower="-6.283185307179586" upper="6.283185307179586" velocity="3.141592653589793" />
  </joint>

  <joint name="joint_2" type="revolute">
    <parent link="shoulder_link" />
    <child link="upper_arm_link" />
    <origin rpy="1.5707 1.5707 0" xyz="0 0 0" />
    <axis xyz="0 0 1" />
    <limit effort="150.0" lower="-6.283185307179586" upper="6.283185307179586" velocity="3.141592653589793" />
  </joint>

  <joint name="joint_3" type="revolute">
    <parent link="upper_arm_link" />
    <child link="forearm_link" />
    <origin rpy="0 0 0" xyz="-0.425 0 0" />
    <axis xyz="0 0 1" />
    <limit effort="150.0" lower="-3.141592653589793" upper="3.141592653589793" velocity="3.141592653589793" />
  </joint>

  <joint name="joint_4" type="revolute">
    <parent link="forearm_link" />
    <child link="wrist_1_link" />
    <origin rpy="0 0 -1.5707" xyz="-0.3922 0 0.1333" />
    <axis xyz="0 0 1" />
    <limit effort="28.0" lower="-6.283185307179586" upper="6.283185307179586" velocity="3.141592653589793" />
  </joint>

  <joint name="joint_5" type="revolute">
    <parent link="wrist_1_link" />
    <child link="wrist_2_link" />
    <origin rpy="1.570796327 0 0" xyz="0 -0.0997 0" />
    <axis xyz="0 0 1" />
    <limit effort="28.0" lower="-6.283185307179586" upper="6.283185307179586" velocity="3.141592653589793" />
  </joint>

  <joint name="joint_6" type="revolute">
    <parent link="wrist_2_link" />
    <child link="wrist_3_link" />
    <origin rpy="1.570796326589793 3.141592653589793 3.141592653589793" xyz="0 0.0996 0" />
    <axis xyz="0 0 1" />
    <limit effort="28.0" lower="-6.283185307179586" upper="6.283185307179586" velocity="3.141592653589793" />
  </joint>

  <!-- Gazebo properties for the arm -->
  <gazebo reference="base_link">
    <material>Gazebo/LightGrey</material>
    <static>false</static>
  </gazebo>
  
  <gazebo reference="shoulder_link">
    <material>Gazebo/LightGrey</material>
  </gazebo>
  
  <gazebo reference="upper_arm_link">
    <material>Gazebo/LightGrey</material>
  </gazebo>
  
  <gazebo reference="forearm_link">
    <material>Gazebo/LightGrey</material>
  </gazebo>
  
  <gazebo reference="wrist_1_link">
    <material>Gazebo/LightGrey</material>
  </gazebo>
  
  <gazebo reference="wrist_2_link">
    <material>Gazebo/LightGrey</material>
